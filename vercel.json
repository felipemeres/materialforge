{
  "functions": {
    "api/config.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/app.js",
      "destination": "/app.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}